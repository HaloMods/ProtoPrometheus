          </bit>
          <bit index="1" name="additive" />
          <bit index="2" name="always active">
            <tooltip>function does not deactivate when at or below lower bound</tooltip>
          </bit>
        </value>
        <value type="Real" name="period">
          <tooltip>this is the period for the above function (lower values make the function oscillate quickly, higher values make it oscillate slowly)</tooltip>
