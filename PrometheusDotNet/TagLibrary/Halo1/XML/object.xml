          <item value="8" name="noise" />
          <item value="9" name="jitter" />
          <item value="10" name="wander" />
          <item value="11" name="spark" />
        </value>
        <value type="Real" name="wobble period">
          <tooltip>the length of time it takes for the magnitude of this function to complete a wobble</tooltip>
        </value>
        <value type="Real" name="wobble magnitude">
          <tooltip>the amount of random wobble in the magnitude</tooltip>
        </value>
        <value type="RealFraction" name="square wave threshold">
          <tooltip>if non-zero, all values above the square wave threshold are snapped to 1.0, and all values below it are snapped to 0.0 to create a square wave.</tooltip>
        </value>
        <value type="ShortInteger" name="step count">
          <tooltip>the number of discrete values to snap to (e.g., a step count of 5 would snap the function to 0.00,0.25,0.50,0.75 or 1.00)</tooltip>
        </value>
        <value type="Enum" name="map to">
          <item value="0" name="linear" />
          <item value="1" name="early" />
          <item value="2" name="very early" />
          <item value="3" name="late" />
          <item value="4" name="very late" />
          <item value="5" name="cosine" />
        </value>
        <value type="ShortInteger" name="sawtooth count">
          <tooltip>the number of times this function should repeat (e.g., a sawtooth count of 5 would give the function a value of 1.0 at each of 0.25,0.50,0.75 as well as at 1.0</tooltip>
        </value>
        <value type="Enum" name="add">
          <tooltip>add this function to the final result of all of the above math</tooltip>
          <item value="0" name="none" />
          <item value="1" name="A in" />
          <item value="2" name="B in" />
          <item value="3" name="C in" />
          <item value="4" name="D in" />
          <item value="5" name="A out" />
          <item value="6" name="B out" />
          <item value="7" name="C out" />
          <item value="8" name="D out" />
        </value>
        <value type="Enum" name="scale result by">
